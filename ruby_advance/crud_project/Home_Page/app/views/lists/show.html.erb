<div class="well col-xs-8 col-xs-offset-2" id="show">
    <h4 class="center"><strong>Id : <%= @list.id%></strong></h4>
    <hr>
    <h4 class="center"><strong>Name : <%= @list.name%></strong></h4>
    <hr>
    <h4 class="center"><strong>Roll : <%= @list.roll%></strong></h4>
    <hr>  
    <h4 class="center"><strong>Grade : <%= @list.grade%> </strong></h4>
    <% if logged_in? && current_user == @list.user %>
    <hr>   
    <div class="article-actions">
        <%= link_to "Edit this record" ,  edit_list_path , class: "btn btn-primary btn-sm btn-xlarge " %>
        <%= link_to "Show this record" ,  lists_path , class: "btn btn-succes btn-sm btn-xlarge" %>
        <%= link_to "Back to record listing" , lists_path , class: "btn btn-info btn-sm btn-xlarge" %>
        <%# <%= button_to "Delete this record", lists_path(@listl), data: { turbo_method: :delete } , class:"btn btn-danger btn-sm btn-xlarge "%> 
    </div>
    <% end %>
</div>
